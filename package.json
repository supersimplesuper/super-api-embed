{
  "name": "@super-api/super-api-embed",
  "version": "3.0.1",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.3"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supersimplesuper/super-api-embed.git"
  },
  "scripts": {
    "build": "npm run clean && npm run type:dts && npm run type:extract && npm run build:src",
    "build:src": "node --import=@nitrogql/esbuild-register ./scripts/build.ts",
    "clean": "rimraf build coverage",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:extract": "api-extractor run && rimraf --glob \"./build/*\" && mv trimmed.d.ts build/index.d.ts",
    "type:check": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.9",
    "@nitrogql/esbuild-register": "^1.7.0",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/jest": "^29.5.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "c8": "^10.1.2",
    "cspell": "^8.15.2",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "loglevel": "^1.9.2"
  }
}
